<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entry.Plan" table="plan">
        <id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="planid" column="planid"/>
		<property name="disciplineid" column="disciplineid"/>
		<property name="lessontypeid" column="lessontypeid"/>
        <property name="empid" column="empid"/>
        <property name="periodid" column="periodid"/>
        <property name="hours" column="hours"/>
        <property name="marktypeid" column="marktypeid"/>
        <property name="modified" column="modified"/>
        <property name="created" column="created"/>
    </class>
</hibernate-mapping>
